{
    "name": "vscode-atomizer-theme",
    "displayName": "Atomizer (Atom One Dark theme)",
    "description": "A detailed and accurate Atom One Dark Theme",
    "version": "1.0.1",
    "publisher": "riipandi",
    "icon": "images/icon.png",
    "author": {
        "name": "riipandi"
    },
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "color",
        "theme",
        "atom",
        "dark",
        "one dark"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Atomizer",
                "uiTheme": "vs-dark",
                "path": "./themes/Atomizer-color-theme.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#17293C",
        "theme": "dark"
    },
    "scripts": {
        "fix": "prettier '**/*.{json,md,yml}' --write",
        "lint": "prettier '**/*.{json,md,yml}' --check"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run lint"
        }
    },
    "lint-staged": {
        "**/*.{json,md,yml}": "npm run fix"
    },
    "bugs": {
        "url": "https://github.com/riipandi/vscode-atomizer-theme/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/riipandi/vscode-atomizer-theme.git"
    },
    "homepage": "https://github.com/riipandi/vscode-atomizer-theme/blob/master/readme.md",
    "devDependencies": {
        "husky": "4.2.5",
        "lint-staged": "10.2.11",
        "prettier": "2.0.5"
    },
    "__metadata": {
        "id": "44ccc6f3-7438-4c16-9740-236dbf63bfc9",
