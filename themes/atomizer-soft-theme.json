    "tokenColors": [
        {
            "name": "New keyword, logical operator keyword",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#ba8ef7"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#229977"
            }
        },
        {
            "name": "Comment Shebang",
            "scope": [
                "comment.line.shebang"
            ],
            "settings": {
                "foreground": "#ff8ad1"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ba8ef7"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#ba8ef7"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.language",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#ffa763"
            }
        },
        {
            "name": "Punctuation template expression",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.definition.typeparameters",
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Tag, Misc",
            "scope": [
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Builtin function",
            "scope": [
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#89ddff"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "Component Tag",
            "scope": [
                "support.class.component"
            ],
            "settings": {
                "foreground": "#ba8ef7"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#f7768e"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "punctuation.definition.arguments",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "punctuation.accessor.js",
