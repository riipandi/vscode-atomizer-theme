                "punctuation.accessor.js",
                "punctuation.parenthesis"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#14b8a6"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#89ddff"
            }
        },
        {
            "name": "Constant, Function Argument, Tag Attribute, Embedded",
